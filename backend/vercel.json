{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "GEMINI_API_KEY": "GEMINI_API_KEY",  
      "DATABASE_URL": "DATABASE_URL",       
      "JWT_SECRET": "JWT_SECRET"
    },
    "routes": [
      {
        "src": "/api/scrape",
        "dest": "/index.js"
      },
      {
        "src": "/api/query",
        "dest": "/index.js"
      },
      {
        "src": "(.*)", 
        "dest": "/index.js"
      }
    ]
  }
  