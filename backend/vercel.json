{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "GEMINI_API_KEY": "@your-gemini-api-key",  
      "DATABASE_URL": "@your-database-url"       
    },
    "routes": [
      {
        "src": "/api/scrape",
        "dest": "/index.js"
      },
      {
        "src": "/api/query",
        "dest": "/index.js"
      },
      {
        "src": "(.*)", 
        "dest": "/index.js"
      }
    ]
  }
  